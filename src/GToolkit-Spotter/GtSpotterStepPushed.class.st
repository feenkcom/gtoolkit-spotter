Class {
	#name : #GtSpotterStepPushed,
	#superclass : #GtSpotterAnnouncement,
	#instVars : [
		'spotterStep'
	],
	#category : #'GToolkit-Spotter-Announcements'
}

{ #category : #accessing }
GtSpotterStepPushed >> spotterStep [

	^ spotterStep
]

{ #category : #accessing }
GtSpotterStepPushed >> spotterStep: anObject [

	spotterStep := anObject
]
