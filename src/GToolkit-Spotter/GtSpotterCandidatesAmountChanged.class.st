Class {
	#name : 'GtSpotterCandidatesAmountChanged',
	#superclass : 'GlamorousAuthor',
	#instVars : [
		'amount'
	],
	#category : 'GToolkit-Spotter-Announcements'
}

{ #category : 'accessing' }
GtSpotterCandidatesAmountChanged >> amount [
	^ amount
]

{ #category : 'accessing' }
GtSpotterCandidatesAmountChanged >> amount: anObject [
	amount := anObject
]
